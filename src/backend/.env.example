NODE_ENV=development
API_PORT=8080
DEBUG_MODE=true
AWS_REGION=ap-southeast-1

# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=trading_bot
DB_PASSWORD=your_secure_password
DB_NAME=trading_bot_db
DB_POOL_SIZE=10
DB_SSL_MODE=false
DB_REPLICA_HOST=replica.localhost
DB_REPLICA_PORT=5432

# TimescaleDB Configuration
TIMESCALE_HOST=localhost
TIMESCALE_PORT=5432
TIMESCALE_USERNAME=timescale_user
TIMESCALE_PASSWORD=your_secure_password
TIMESCALE_DB_NAME=trading_bot_timeseries

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=your_secure_password

# Security Configuration
JWT_SECRET_KEY=your_jwt_secret_key_min_32_chars
JWT_ACCESS_TOKEN_EXPIRY=3600
JWT_REFRESH_TOKEN_EXPIRY=604800
KMS_KEY_ID=arn:aws:kms:ap-southeast-1:your-account-id:key/your-key-id
KMS_REGION=ap-southeast-1

# External API Configuration
JUPITER_API_ENDPOINT=https://api.jup.ag/v1
JUPITER_API_KEY=your_jupiter_api_key
PUMP_FUN_API_ENDPOINT=https://api.pump.fun/v1
PUMP_FUN_API_KEY=your_pump_fun_api_key
DRIFT_API_ENDPOINT=https://api.drift.trade/v1
DRIFT_API_KEY=your_drift_api_key
JITO_API_ENDPOINT=https://api.jito.wtf/v1
JITO_API_KEY=your_jito_api_key

# Monitoring Configuration
PROMETHEUS_ENDPOINT=http://localhost:9090
GRAFANA_API_KEY=your_grafana_api_key

# Additional Configuration
LOG_LEVEL=debug
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8080
REQUEST_TIMEOUT_MS=30000
MAX_CONNECTIONS=1000